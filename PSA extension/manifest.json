{
  "name": "PSA Extension",
  "description": "Remplis le formulaire PSA Time",
  "version": "1.1",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "CGI.png"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "all_frames": true,
      "js": [
        "page.js",
        "background.js"
      ]
    }
  ]
  
}